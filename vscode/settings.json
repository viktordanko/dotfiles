{
  "editor.minimap.enabled": false,
  "editor.insertSpaces": false,
  "material-icon-theme.folders.theme": "classic",
  "material-icon-theme.hidesExplorerArrows": true,
  "material-icon-theme.folders.color": "#42a5f5",
  "workbench.iconTheme": "material-icon-theme",
  "breadcrumbs.enabled": true,
  "emmet.includeLanguages": {
    "nunjucks": "html",
    "latte": "html",
    "javascript": "javascriptreact"
  },
  "html.format.endWithNewline": true,
  "html.format.wrapAttributes": "force-expand-multiline",
  "window.zoomLevel": -2,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.wordWrap": "on",
  "editor.renderWhitespace": "all",
  "editor.renderControlCharacters": true,
  "files.trimTrailingWhitespace": true,
  "editor.suggestSelection": "first",
  "editor.suggest.showSnippets": true,
  "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?_",
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "html.format.enable": false,
  "editor.tabCompletion": "onlySnippets",
  "gitlens.keymap": "none",
  "gitlens.advanced.messages": {
    "suppressLineUncommittedWarning": true
  },
  "gitlens.hovers.currentLine.over": "line",
  "sync.gist": "01645042ee99f810e702796884e309fc",
  "yaml.schemas": {
    "file:///Users/viktordanko/.vscode/extensions/atlassian.atlascode-1.4.1/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "terminal.integrated.rendererType": "dom",
  "terminal.integrated.fontFamily": "JetBrains Mono",
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontLigatures": true,
  "workbench.fontAliasing": "antialiased",

  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[typescriptreact]": {
    "editor.formatOnSave": true
  },
  "files.watcherExclude": {
    "\"/.git\": true": true
  },
  "files.associations": {
    "*.md": "mdx"
  },
  "editor.formatOnPaste": true,
  "editor.formatOnType": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true,
    "source.addMissingImports": true
  },
  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5",
  "git.autofetch": true,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "search.exclude": {
    "**/build": true,
    "**/dist": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.accessibilitySupport": "off",
  "editor.fontSize": 14,
  "liveshare.featureSet": "insiders",
  "vsicons.dontShowNewVersionMessage": true,
  "editor.formatOnSave": true,
  "editor.renameOnType": true,
  "launch": {
    "inputs": []
  },
  "workbench.startupEditor": "welcomePage",
  "workbench.colorTheme": "SUPERKODERS Theme"
}
